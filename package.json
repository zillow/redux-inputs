{
  "name": "redux-inputs",
  "version": "2.5.4",
  "description": "Redux actions and reducers for managing and validating forms and inputs",
  "homepage": "https://github.com/zillow/redux-inputs",
  "repository": {
    "type": "git",
    "url": "https://github.com/zillow/redux-inputs"
  },
  "files": [
    "lib",
    "esm"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "prepare": "npm run build && npm run test",
    "test": "npm run jest && npm run lint",
    "jest": "jest --coverage",
    "lint": "eslint src/*.js src/**/*.js __tests__/**/*.js",
    "build:cjs": "BABEL_ENV=cjs babel src/ -d lib/",
    "build:esm": "BABEL_ENV=esm babel src/ -d esm/",
    "build": "npm run build:cjs && npm run build:esm",
    "build-examples": "webpack --config examples/webpack.config.js",
    "watch-examples": "webpack --watch --config docs/examples/webpack.config.js",
    "serve-examples": "browser-sync start --server --port 3001 --files docs/examples/*/out.js --startPath /docs/examples/"
  },
  "keywords": [
    "react",
    "redux",
    "form",
    "inputs",
    "validation",
    "submit"
  ],
  "contributors": [
    "Jake Pusateri <jacobp@zillow.com>",
    "Steven Bierstiker <stevenb@zillow.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.2.0",
    "invariant": "^2.2.0",
    "lodash": "^4.5.1",
    "prop-types": "^15.5.8",
    "react-display-name": "^0.2.0",
    "react-redux": "^4.4.5 || ^5.0.0",
    "redux": "^3.5.2",
    "reselect": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.17.5",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^23.6.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "redux-mock-store": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "sinon": "^7.2.2",
    "webpack": "^4.28.4"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "redux-thunk": "^2.0.0"
  },
  "jest": {
    "testURL": "http://localhost/"
  }
}
